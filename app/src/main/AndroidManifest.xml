<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.ulpgc.juan.exampemandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="es.ulpgc.juan.exampemandroid.app.AppMediator"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="es.ulpgc.juan.exampemandroid.master.MasterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="es.ulpgc.juan.exampemandroid.add.AddActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="es.ulpgc.juan.exampemandroid.Detail.DetailActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
    </application>

</manifest>